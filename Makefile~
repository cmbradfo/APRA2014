# latex Makefile
LATEX=latex
BIBTEX=bibtex
DVIPS=dvips
TARGETS= ms.ps
#ms.pdf

all: $(TARGETS)

clean: 
	@rm -f *.aux *.bbl *.blg *.dvi *.log

ms.dvi: ms.tex
	${LATEX} ms.tex

ms.ps: ms.dvi 
	${BIBTEX} ms
	${LATEX} ms.tex
	${BIBTEX} ms
	${LATEX} ms.tex
	${DVIPS} ms.dvi -o ms.ps

ms.pdf: ms.ps
	ps2pdf bgps.ps














